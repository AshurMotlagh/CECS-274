import java.util.Random;
public class DiceGame{
    public static int dice3Roll(){
        Random rand = new Random();
        int x = rand.nextInt(18-3+1) + 3; // rand(max-min + 1) + min <------ formula for rand ints
        return x;

    }
    public static void main(String [] args){
        int diceValue;
        for(int i=0, i<12, i++){
            diceValue = dice3Roll();
            System.out.println(diceValue);
        }
    }
}